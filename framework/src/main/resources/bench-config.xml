<bench-config>

   <master bind="${bind.address}" slavesCount="${cbf.nodes.count}"/>

   <scaling-benchmark initSize="${cbf.nodes.initsize}" maxSize="${cbf.nodes.count}" increment="${cbf.nodes.increment}" name="${cbf.test.name}">
      <benchmark-stages>
         <stage name="StartClusterStage">
            <property name="config" value="${cbf.test.config}" mapAggregator="wrapperStartupParams"/>
         </stage>
         <stage name="ClusterValidationStage">
            <property name="PartialReplication" value="false"/>
         </stage>
         <stage name="WarmupStage">
            <property name="OperationCount" value="10000"/>
         </stage>
         <stage name="WebSessionBenchmarkStage">
            <property name="numberOfRequests" value="500000"/>
         </stage>
         <stage name="CsvReportGenerationStage"/>
         <stage name="BenchmarkCleanupStage"/>
      </benchmark-stages>
   </scaling-benchmark>
</bench-config>
